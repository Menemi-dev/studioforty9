{
  "name": "theme-build",
  "version": "1.0.0",
  "description": "A theme built scripting",
  "private": true,
  "targets": {
    "default": {
      "distDir": "assets"
    }
  },
  "scripts": {
    "build": "parcel build src/js/main.js --no-source-maps",
    "watch": "parcel watch src/js/main.js --no-source-maps --no-hmr",
    "watch:serve": "concurrently \"parcel watch src/js/main.js --no-source-maps --no-hmr\" \"shopify theme serve\""
  },
  "keywords": [],
  "author": "Emilia Mencia",
  "license": "ISC",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.0.1",
    "concurrently": "^7.0.0",
    "parcel": "^2.0.1"
  }
}
